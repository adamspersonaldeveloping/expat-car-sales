<div class="container pt-4">
  <div class="body-light">
    <span class="text-light">Wanna join our email list?</span>
    <form action="/addEmailToList" method="POST">
      <div class="mb-3 row d-flex flex-wrap">
        <div class="col-10 col-sm-4">
          <label for="newEmail" class="form-label text-secondary">Email</label>
          <input type="text" class="form-control" id="newEmail" name="newEmail" placeholder="email@email.com" required>
        </div>
        <div class="col-10 col-sm-4">
          <label for="firstName" class="form-label text-secondary">Frist Name</label>
          <input type="text" class="col-3 form-control" id="firstName" name="firstName" placeholder="i.e. John" required>
        </div>

        <div class="col-10 col-sm-4">
          <label for="lastName" class="form-label text-secondary">Last Name</label>
          <input type="text" class="col-3 form-control" id="lastName" name="lastName" placeholder="i.e. Smith" required>
        </div>
        <button type="submit" class="ms-1 mt-3 col-4 btn btn-outline-info btn-lg px-4 me-sm-3 fw-bold" value="Upload">Subscribe</button>
      </div>
    </form>
  </div>
