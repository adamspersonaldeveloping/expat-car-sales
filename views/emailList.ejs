<%- include('partials/header') -%>


<div class="container">
    <h2 class="text-white">Here are all of the people on your email list:</h2>
    <ul class="row list-unstyled">
        <% for(var i=0; i<emaillists.length; i++) {%>
          
          <li class="col-6 justify-content-between mt-5 border border-light rounded">
            <h4 class="text-white justify-content-center">First Name: <%= emaillists[i].firstName %></h4>
            <h4 class="text-white justify-content-center">Last Name: <%= emaillists[i].lastName %></h4>
            <h2 class="text-white justify-content-center">Email: <%= emaillists[i].email %></h2>
            
          </li>
        
        <% } %>
        </ul>
</div>

<%- include('partials/footer') -%>